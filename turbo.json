{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["build"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:ci": {
      "dependsOn": ["build"],
      "cache": false
    },
    "e2e": {},
    "check": {
      "cache": false
    },
    "check:write": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
